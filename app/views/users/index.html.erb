<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th><h2>Name</h2></th>
      <th><h2>Last Sign In</h2></th>
      <th><h2>Member Since<h2></th>
      <th><h2>Last Signed In From</h2></th>
    </tr>
  <thead>
  <tbody>
    <% for user in @users %>
      <tr>
        <td><h3><%= link_to user.name, user %></h3></td>
        <td><h3><%= user.last_sign_in_at %><h3></td>
        <td><h3><%= user.created_at.to_date %></h3></td>
        <td><h3><%= user.last_sign_in_ip %></h3></td>
      </tr>
    <% end %>
  </tbody>
</table>
